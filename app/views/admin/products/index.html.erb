
<table class='table table-hover table-bordered'>
  <thead>
  <tr>
    <td>Title</td>
    <td>Description</td>
    <td>Quantity</td>
    <td>Image]</td>
  </tr>
</thead>
  <%@products.each do |product| %>
  <tbody>
  <tr>
    <td><%= product.title %></td>
    <td><%= product.description %></td>
    <td><%= product.quantity %></td>
    <td><%= product.image.url.to_s%></td>
  </tr>
</tbody>
<% end %>
</table>